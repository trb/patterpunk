# You only need to set the credentials for the providers you'll use. E.g. if you're using
# AWS Bedrock you won't need to set open ais key.
OPENAI_API_KEY=

# Set this if you want to use Azure OpenAI Responses API (v1)
# Note: Azure uses deployment names (created in Azure Portal) instead of model names
# Endpoint format: https://YOUR-RESOURCE-NAME.openai.azure.com/openai/v1/
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/openai/v1/
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_REASONING_ENDPOINT=https://your-resource-eastus2.cognitiveservices.azure.com/
AZURE_OPENAI_REASONING_API_KEY=

# Set this if you want to use AWS Bedrock
AWS_ACCESS_KEY_ID=aws access key id
AWS_SECRET_ACCESS_KEY=aws secret key

# Set the ollama endpoint to activate ollama support
OLLAMA_API_ENDPOINT=

# Set a anthropic api key to activate anthropic access
ANTHROPIC_API_KEY=

GOOGLE_LOCATION=
# Content of the account key .json file, as a single line string
GOOGLE_APPLICATION_CREDENTIALS=
